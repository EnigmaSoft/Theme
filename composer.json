{
    "name": "enigmasoft/theme",
    "description": "Implements Theme functionality for EnigmaCMS",
    "license": "CC-BY-NC-4.0",
    "authors": [{
        "name": "Snopboy",
        "email": "snopboy@localhost.com"
    }],
    "require": {
        "illuminate/support": "^5.5"
    },
    "autoload": {
        "files": [
            "src/Helpers.php"
        ],
        "psr-4": {
            "Enigma\\Theme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enigma\\Theme\\ThemeServiceProvider"
            ],
            "aliases": {
                "Theme": "Enigma\\Theme\\Facades\\ThemeFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}